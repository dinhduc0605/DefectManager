<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nguyendinhduc.myapplication" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DetailIssueActivity"
            android:label="@string/title_activity_detail_issue"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".DetailAccountActivity"
            android:label="@string/title_activity_detail_account" >
        </activity>
        <activity
            android:name=".EditAccountActivity"
            android:label="@string/title_activity_edit_account" >
        </activity>
        <activity android:name=".CreateAccountActivity"
            android:label="@string/title_activity_create_account">
        </activity>
        <activity android:name=".DetailProjectActivity"
            android:label="@string/title_activity_detail_project">
        </activity>
        <activity android:name=".EditProjectActivity"
            android:label="@string/title_activity_edit_project">
        </activity>
        <activity android:name=".CreateProjectActivity"
            android:label="@string/title_activity_create_project">
        </activity>
        <activity android:name=".EditIssueActivity" android:label="@string/title_activity_edit_issue">
        </activity>
        <activity android:name=".CreateIssueActivity" android:label="@string/title_activity_create_issue" >
        </activity>
    </application>

</manifest>
